---
import BaseLayout from '../layouts/Base.astro';

interface Props {
    title: string;
    description: string;
}

const {
	title,
	description,
} = Astro.props;

const layoutProps = {
    title,
    description,
    permalink: '',
    image: '',
    bodyClass: 'page',
}
---
<BaseLayout {...layoutProps}>
    <slot />
</BaseLayout>
